algoritmo "Operador de escolha"

var
voto : literal

inicio

Escreval("Digite seu voto, Opções: 20, 30, 40")
leia(voto)

Escolha voto
caso "20"
   Escreval("Você votou em Marcelo Bernart.")
caso "30"
   Escreval("Você votou em Pernalonga.")
caso "40"
   Escreval("Você votou em Patolino.")
outrocaso
   Escreval("Voto nulo.")
fimescolha

fimalgoritmo
